<decorators defaultdir="/decorators">
	<excludes>
		<pattern>*.js</pattern>
		<pattern>*.css</pattern>
		<pattern>/40*.jsp</pattern>
		<pattern>/*ajax=true*</pattern>
		<pattern>/scripts/dojo/*</pattern>
		<pattern>/struts/dojo/*</pattern>
		<pattern>/resources/*</pattern>
		<pattern>/common/*</pattern>

		<pattern>/WEB-INF/content/cadshow/*</pattern>
		<pattern>/cadshow/*</pattern>
		<pattern>/scada/*</pattern>

		<pattern>/left.*</pattern>
		<pattern>/scada/left_scada.*</pattern>
		<pattern>/checklogin.action</pattern>
		<pattern>/splitter.*</pattern>
		<pattern>/toindex.*</pattern>
		<pattern>/top.*</pattern>
		<pattern>/top_swich.*</pattern>
		<pattern>*/login.*</pattern>
		<pattern>/index.*</pattern>
		<pattern>/login-check.*</pattern>
		<pattern>/toolbar/*</pattern>
		<pattern>/js/*</pattern>


		<pattern>/WEB-INF/content/readmedia/*</pattern>
		<pattern>/readmedia/*</pattern>



		<pattern>/WEB-INF/content/defect/0/*</pattern>
		<pattern>/defect/defect!read0.action*</pattern>

		<pattern>/WEB-INF/content/equip/0/*</pattern>
		<pattern>/equip/pms-gzxx!read0.action*</pattern>
		<pattern>/equip/pms-jxxx!read0.action*</pattern>
		<pattern>/equip/pms-qxxx!read0.action*</pattern>

		<pattern>/WEB-INF/content/check/0/*</pattern>
		<pattern>/check/check-equip!read0.action*</pattern>
		
		<pattern>/WEB-INF/content/xianchang/0/*</pattern>
	    <pattern>/xianchang/xianchang-img!input.action*</pattern>
		
		<pattern>/init_password.jsp*</pattern>
		<pattern>/initPassword.action*</pattern>
	 
	</excludes>
	

	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
